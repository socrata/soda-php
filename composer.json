{
    "name": "socrata/soda-php",
    "description": "A simple library to make it easier to access SODA services from PHP",
    "keywords": ["SODA", "socrata"],
    "homepage": "http://dev.socrata.com",
    "require": {
        "php": "~5.3",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable"
    },
    "autoload": {
        "psr-4": {
            "socrata\\soda\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "socrata\\soda\\test\\": "test"
        }
    }
}
